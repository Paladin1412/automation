					<include name="agreeContractTerms2" description="我同意上述合同条款2" />
					<include name="okContract" description="确认合同" />
					<include name="okAboveInfo1" description="我确认以上信息1" />
					<include name="okAboveInfo2" description="我确认以上信息2" />
					<include name="verificationOk" description="点击验证码确认" /> 
					<include name="getPayInfo" description="验证码后获取支付信息" />
					<include name="ziroom_fistPayMent" description="自如支付" />
				</methods>
			</class>
			
		</classes>
	</test>
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter"/>
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
	</listeners>

